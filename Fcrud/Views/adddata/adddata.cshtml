


@model Fcrud.Models.StuModel

@using (Html.BeginForm("adddata", "adddata", FormMethod.Post))
{

    <div class="form-group">
        <label>ID</label>
        @Html.TextBoxFor(m => m.Id, new { @class = "form-control", @required = "required" })

    </div>
    <div class="form-group">
        <label>Name</label>
        @Html.TextBoxFor(m => m.name, new { @class = "form-control", @required = "required" })

    </div>
    <div class="form-group">
        <label>AGE</label>
        @Html.TextBoxFor(m => m.age, new { @class = "form-control", @required = "required" })

    </div>
    <div class="form-group">
        <label>Company nameD</label>
        @Html.TextBoxFor(m => m.company, new { @class = "form-control", @required = "required" })

    </div>
    <div class="form-group">
        <label>Email</label>
        @Html.TextBoxFor(m => m.email, new { @class = "form-control", @required = "required" })

    </div>

    <button type="submit" class="btn-success">SUBMIT</button>


    <br />
    <div class="mb-3 row">
        <label for="staticEmail" class="col-sm-2 col-form-label">Email</label>
        <div class="col-sm-10">
            <input type="text" readonly class="form-control-plaintext" id="staticEmail" value=" ">
        </div>
    </div>
    <div class="mb-3 row">
        <label for="inputPassword" class="col-sm-2 col-form-label">Password</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="inputPassword">
        </div>
    </div>

}

